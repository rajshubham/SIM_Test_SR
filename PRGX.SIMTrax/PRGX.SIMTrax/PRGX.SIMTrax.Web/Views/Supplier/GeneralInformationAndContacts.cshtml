@model PRGX.SIMTrax.ViewModel.SellerRegister



<style>
    div.fileinputs {
        position: relative;
    }

    div.fakefile {
        position: absolute;
        top: 0px;
        left: 0px;
        z-index: 1;
    }

        div.fakefile input[type=button] {
            /* enough width to completely overlap the real hidden file control */
            cursor: pointer;
            width: 148px;
        }

    div.fileinputs input.file {
        position: relative;
        text-align: right;
        -moz-opacity: 0;
        filter: alpha(opacity: 0);
        opacity: 0;
        z-index: 2;
        height: 34px;
        cursor: pointer;
        left: -100px;
    }

    .disabled {
        opacity: 0.4;
        filter: alpha(opacity=40);
    }

    .progress-bar-success {
        filter: none;
    }

    #demoTree li {
        margin-top: 10px !important;
    }

    .padding-left-44 {
        padding-left: 44px;
    }

    .padding-left-42 {
        padding-left: 42px;
    }

    .font-size-22 {
        font-size: 22px;
    }


    .body-no-scroll {
        overflow: hidden !important;
    }

    .SICCodeLink {
        color: #449d44;
    }

    a.SICCodeLink:hover {
        text-decoration: underline !important;
        color: #449d44 !important;
    }


    .next-previous-icons {
        height: 60px;
        font-size: 20px;
        padding-top: 20px;
        cursor: pointer;
    }

    .padding-top-8 {
        padding-top: 8px;
    }

    .edit-delete-icons {
        padding-left: 8px;
        font-size: 20px;
        cursor: pointer;
    }

    #countOfCompleted {
        padding-left: 29px;
    }

    .padding-left-0 {
        padding-left: 0px;
    }

    .buttons-outside-container {
        position: absolute;
        top: -94px;
    }

    @@media (max-width: 430px) {
        #contacts-list-table {
            overflow-x: scroll;
        }
    }

    @@media (max-width: 990px) {
        .padding-left-right-0px {
            padding-left: 0px;
            padding-right: 0px;
        }
    }
</style>
