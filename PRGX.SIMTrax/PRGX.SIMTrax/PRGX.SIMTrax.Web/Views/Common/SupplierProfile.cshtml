@model PRGX.SIMTrax.Domain.Model.Profile
@{
    var userCulture = (Session[PRGX.SIMTrax.Domain.Util.Constants.USER_PREFERENCE_CULTURE] != null) ? (System.Globalization.CultureInfo)Session[PRGX.SIMTrax.Domain.Util.Constants.USER_PREFERENCE_CULTURE] : PRGX.SIMTrax.Web.Controllers.BaseController.ResolveCulture();
    ViewBag.Title = PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.PROFILE, userCulture).ToString();
}

@{
    var UserType = (Session[PRGX.SIMTrax.Domain.Util.Constants.SESSION_USER_TYPE] != null)
               ? (PRGX.SIMTrax.Domain.Util.UserType)Convert.ToInt64(Session[PRGX.SIMTrax.Domain.Util.Constants.SESSION_USER_TYPE]) : PRGX.SIMTrax.Domain.Util.UserType.None;
    var buyerPermissionList = (Session[PRGX.SIMTrax.Domain.Util.Constants.SESSION_BUYER_ACCESS_PERMISSIONS] != null) ? ((List<PRGX.SIMTrax.Domain.Util.ItemList>)Session[PRGX.SIMTrax.Domain.Util.Constants.SESSION_BUYER_ACCESS_PERMISSIONS]) : new List<PRGX.SIMTrax.Domain.Util.ItemList>();
    var IsGeneralInformationNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.GeneralInformation) != null) ? true : false;
    var IsContactsNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.Contacts) != null) ? true : false;
    var IsAddressNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.Addresses) != null) ? true : false;
    var IsComplianceStatusNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.ComplianceStatus) != null) ? true : false;
    var IsBrowseAnswersNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.FiltersResults) != null) ? true : false;
    var IsInboxNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.Inbox) != null) ? true : false;
    var IsReferenceNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.References) != null) ? true : false;
    var IsBankDetailsNeeded = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.BankDetails) != null) ? true : false;
    var IsSearchNeededForBuyer = (buyerPermissionList.Find(a => a.Value == (long)PRGX.SIMTrax.Domain.Util.BuyerPermissions.Search) != null) ? true : false;
}

@Styles.Render("~/bundles/ChartsCss")
@Styles.Render("~/bundles/ProfileCss")

<div id="companyProfile" class="row">
    <div class="col-md-12">
        @if (UserType == PRGX.SIMTrax.Domain.Util.UserType.Buyer || UserType == PRGX.SIMTrax.Domain.Util.UserType.AdminBuyer)
        {
            <div class="bread-crumb" id="buyer-supplier-profile-bread-crumb">
                <a href="/Buyer/Home" data-title="Home">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.HOME, userCulture).ToString()</a> > <a href="/Buyer/SupplierSearch">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.SEARCH, userCulture).ToString()</a> ><span class="breadCrumbCompanyName" style="padding-left:6px;">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.PROFILE, userCulture).ToString()</span>
            </div>
        }
        else if (UserType == PRGX.SIMTrax.Domain.Util.UserType.AdminAuditor || UserType == PRGX.SIMTrax.Domain.Util.UserType.Auditor)
        {
            <div class="bread-crumb" id="supplier-profile-bread-crumb">
                <a href="/Admin/Home" data-title="Home">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.HOME, userCulture).ToString()</a> > <span class="breadCrumbCompanyName">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.PROFILE, userCulture).ToString()</span>
            </div>
        }
        else
        {
            @*<div class="bread-crumb">
                    <a class="Navigate" data-viewname="supplierHome" data-action="/supplier-home" data-title="Home">Home</a> > <span class="breadCrumbCompanyName">Profile</span>
                </div>*@
        }
    </div>
    @Html.HiddenFor(m => m.SellerPartyId)
    @Html.HiddenFor(m => m.Status, new { id = "profileCompanyStatus" })
    <div class="col-md-6 col-sm-12 col-xs-12 padding-bottom-header">
        <h3 id="profileHeader">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.PROFILE, userCulture).ToString()</h3>
    </div>
    @if (UserType == PRGX.SIMTrax.Domain.Util.UserType.AdminBuyer || UserType == PRGX.SIMTrax.Domain.Util.UserType.Buyer)
    {
        <div class="col-md-6 col-sm-12 col-xs-12 text-align-right font-size-16px padding-top-10px" style="padding-bottom: 15px">
            <div class="profile-action-div">
                @if (IsSearchNeededForBuyer)
                    {
                        <div style="width:86px" class="profile-action-child-div">
                            <input type="checkbox" class="supplier-type-checkbox buyer-action-color" value="1" id="markAsFavSup" />
                            <label for="markAsFavSup" class="font-normal buyer-action-color">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.FAVOURITE, userCulture).ToString()</label>
                        </div>
                        <div style="width:109px" class="profile-action-child-div">
                            <input type="checkbox" class="supplier-type-checkbox buyer-action-color" value="2" id="markAsTradSup" />
                            <label for="markAsTradSup" class="font-normal buyer-action-color">@PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.TRADING_WITH, userCulture).ToString()</label>
                        </div>
                    }
                    @if (IsBrowseAnswersNeeded || IsInboxNeeded)
                    {
                        <div style="width:146px" class="profile-action-child-div">
                            <a class="cursor font-normal buyer-action-color" id="viewDiscussionProfile">
                                <span>
                                    <i class="fa fa-envelope"></i><span id="txtViewDiscussionProfile"> @PRGX.SIMTrax.Domain.Util.ReadResource.GetResourceForGlobalization(PRGX.SIMTrax.Domain.Util.Constants.VIEW_DISCUSSION, userCulture).ToString()</span>
                                </span>
