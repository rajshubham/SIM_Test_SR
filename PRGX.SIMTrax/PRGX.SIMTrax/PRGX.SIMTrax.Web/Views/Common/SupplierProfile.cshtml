                        </div>
                        <div id="BusinessDescription" class="padding-top col-md-12 col-sm-12 col-xs-12">
                            @Html.DisplayFor(m => m.BusinessDescription)
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-3 col-sm-4 col-xs-12 padding-bottom-header" id="divLastSubmittedDate">
