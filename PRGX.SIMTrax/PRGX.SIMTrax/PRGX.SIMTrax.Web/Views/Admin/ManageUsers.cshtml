                                    @Html.TextBoxFor(m => m.LoginId, new { id = "tbloginIDEditPassword", @class = "form-control", @readonly = "readonly" })
                                </div>
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 padding-top-10px">
